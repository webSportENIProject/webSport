@model WUI.Models.TypePointModel

@{
    ViewBag.Title = "Edit";
}

@section css {

}

<h2>Edit</h2>

@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    
        @Html.HiddenFor(model => model.Id)

        <div class="form-group">
            @Html.LabelFor(model => model.Libelle)
        </div>
        <div class="form-group">
            @Html.TextBoxFor(model => model.Libelle, new { @class = "form-control" })
            @Html.ValidationMessageFor(model => model.Libelle)
        </div>

        <p>
            <button type="submit" value="Save" class="btn btn-default">Save</button> |
            @Html.ActionLink("Retour à la liste", "Index")
        </p>

}
